{
    "name": "buildamp",
    "version": "0.1.0",
    "description": "Code generation from Elm models",
    "type": "module",
    "main": "./lib/index.js",
    "bin": {
        "buildamp": "./bin/buildamp.js"
    },
    "exports": {
        ".": "./lib/index.js",
        "./core": "./core/index.js",
        "./core/paths": "./core/paths.js",
        "./core/contracts": "./core/contracts.js",
        "./core/parser": "./core/parser.js",
        "./generators": "./lib/generators/index.js"
    },
    "scripts": {
        "test": "node --test tests/*.test.js",
        "test:update-golden": "node tests/update-golden.js"
    },
    "keywords": [
        "elm",
        "codegen",
        "buildamp"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "commander": "^12.0.0",
        "chalk": "^5.3.0",
        "tree-sitter": "^0.25.0",
        "@elm-tooling/tree-sitter-elm": "^5.7.0"
    },
    "engines": {
        "node": ">=20.0.0"
    }
}
