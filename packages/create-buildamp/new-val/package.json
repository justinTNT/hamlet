{
  "name": "new-val",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run build --workspaces",
    "dev": "npm run dev --workspaces",
    "dev:server": "node dev-server.js",
    "generate": "node .buildamp/generate-all.js",
    "generate:watch": "nodemon --watch src/models/ --ext rs --exec 'npm run generate'",
    "dev:frontend": "cd frontend && vite",
    "dev:backend": "cd backend && node --watch server.js"
  },
  "devDependencies": {
    "concurrently": "^8.0.0",
    "nodemon": "^3.0.0"
  },
  "workspaces": [
    "frontend",
    "backend"
  ]
}